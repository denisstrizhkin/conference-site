{% extends "base.jinja" %}

{% block title %}{{title}}{% endblock %}

{% block main %}
<ul>
{% for user in users %}
    <li>{{ user.email }}</li>
{% endfor %}
</ul>

<form action="/user/logout" method="post" class="d-inline">
  <button type="submit" class="btn btn-danger">
    <i class="bi bi-box-arrow-right"></i> Logout
  </button>
</form>

{% endblock %}
